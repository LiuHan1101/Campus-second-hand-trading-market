  padding-bottom: env(safe-area-inset-bottom);
}

/* ===== æ»šåŠ¨æ¡éšè— ===== */
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

/* ===== æ‚é¡¹ä¼˜åŒ– ===== */
.hidden {
  display: none !important;
}

.divider {
  height: 1rpx;
  background: linear-gradient(90deg, transparent, rgba(0, 0, 0, 0.1), transparent);
  margin: 24rpx 0;
}

/* æ¶ˆæ¯çŠ¶æ€æŒ‡ç¤ºå™¨ */
.message-status {
  position: absolute;
  bottom: -36rpx;
  right: 0;
  font-size: 20rpx;
  color: #888;
  display: flex;
  align-items: center;
  gap: 4rpx;
}

.message-sent .message-status {
  right: 100rpx;
}

/* ç½‘ç»œçŠ¶æ€æç¤º */
.network-status {
  position: fixed;
  top: 100rpx;
  left: 50%;
  transform: translateX(-50%);
  background: rgba(0, 0, 0, 0.8);
  color: white;
  padding: 16rpx 32rpx;
  border-radius: 44rpx;
  font-size: 24rpx;
  z-index: 1000;
  backdrop-filter: blur(20px);
  animation: slideDown 0.3s ease-out;
}

/* æ¶ˆæ¯å‘é€åŠ¨ç”» */
@keyframes messageSend {
  0% {
    opacity: 0;
    transform: translateY(20rpx) scale(0.9);
  }
  100% {
    opacity: 1;
    transform: translateY(0) scale(1);
  }
}

.message-item:last-child .message-bubble {
  animation: messageSend 0.4s cubic-bezier(0.34, 1.56, 0.64, 1);
}